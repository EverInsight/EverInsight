  "main": "index.js",
  "scripts": {
    "dev": "watchman watch-del-all && EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start -c -g",
    "build": "eas build --platform all",
    "build:android": "eas build --platform android --local",
    "build:ios": "eas build --platform ios --local",
    "ios": "react-native run-ios",
    "ipad": "react-native run-ios --simulator='iPad (10th generation)'",
    "pod-install": "cd ios && pod install && cd ..",
    "prebuild:ios": "eas build --profile development --platform ios --local"
  },
  "dependencies": {
    "expo": "~50",
    "expo-status-bar": "*",
    "expo-dev-client": "*",
    "expo-system-ui": "*",
    "expo-router": "*",
    "expo-linking": "*",
    "expo-constants": "*",
    "expo-sqlite": "*",
    "expo-splash-screen": "*",
    "expo-file-system": "*",
    "react": "^18.2",
    "react-native": "0.73.4",
    "react-native-screens": "*",
    "react-native-safe-area-context": "4.8.2",
    "react-native-reanimated": "~3.6.2",
    "@faasjs/react": "*",
    "lodash": "*"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@types/react": "^18",
    "@testing-library/react-native": "*",
    "typescript": "*",
    "@preact/signals-react-transform": "*",
    "@types/lodash": "*",
    "eas-cli": "*",
    "jest-expo": "*"
  },
  "engines": {
    "node": ">=20"
  }
